{
  "name": "starwars-application",
  "version": "0.0.0",
  "scripts": {
    "all": "npm run lint && npm run build && npm run test",
    "ng": "ng",
    "start": "ng serve",
    "start:backend": "json-server starwars.json",
    "build": "ng build",
    "build:uitests": "tsc -p tsconfig.uitest.json",
    "build:uitests:watch": "npm run build:uitests -- -w",
    "watch": "ng build --watch --configuration development",
    "test": "npm run test:karma && npm run test:ui",
    "test:karma": "ng test --watch=false --browsers ChromeHeadless",
    "test:karma:watch": "ng test",
    "pretest:ui": "npm run build:uitests",
    "test:ui": "jasmine --config jasmine.json",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "bootstrap": "^5.1.3",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.0",
    "@stryker-mutator/core": "^5.5.0",
    "@stryker-mutator/karma-runner": "^5.5.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.2.0",
    "jasmine": "^3.10.0",
    "jasmine-core": "~3.10.0",
    "json-server": "^0.17.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "playwright": "^1.16.3",
    "tree-kill": "^1.2.2",
    "typescript": "~4.4.3"
  }
}
